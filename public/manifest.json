{
  "manifest_version": 3,
  "name": "IP Changer",
  "version": "2.3.2",
  "description": "A Chrome extension for quickly changing your browser's IP address with a simple interface.",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "IP Changer",
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "options_page": "options/index.html",
  "host_permissions": ["<all_urls>"],
  "permissions": [
    "tabs",
    "proxy",
    "storage",
    "webRequest",
    "webRequestAuthProvider",
    "webNavigation"
  ]
}
